///inputype(index)
//input names for key config

switch (argument[0]) {
    case 0: return lang("up")
    case 1: return lang("down")
    case 2: return lang("left")
    case 3: return lang("right")
    case 4: return lang("jump")
    case 5: return lang("action")
    case 6: return lang("alt")
    case 7: return lang("start")
    case 8: return lang("CUSTOM 1")
    case 9: return lang("CUSTOM 2")
    case 10: return lang("CUSTOM 3")
    case 11: return lang("RESET")
}
